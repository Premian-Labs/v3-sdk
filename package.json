{
	"name": "@premia/v3-sdk-public",
	"version": "0.4.4",
	"description": "The official SDK for building applications on Premia V3",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"files": [
		"dist",
		"src/abi/*.json"
	],
	"author": "research@premia.finance",
	"license": "BSD-3-Clause",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Premian-Labs/premia-v3-sdk.git"
	},
	"keywords": [
		"premia",
		"v3",
		"sdk",
		"api",
		"ethereum",
		"defi",
		"options",
		"trading",
		"and",
		"market-making"
	],
	"scripts": {
		"docs": "npx typedoc src/index.ts",
		"build": "yarn postinstall && tsc",
		"prepublishOnly": "yarn build",
		"test": "npx hardhat test test/unit/* --network anvil",
		"it": "npx hardhat test test/it/* --network anvil",
		"postinstall": "npx typechain --target ethers-v6 --out-dir \"./src/typechain\" \"./src/abi/*.json\"",
		"e2e": "npx hardhat test test/e2e/orderbook.spec.ts --network arbitrumGoerli"
	},
	"dependencies": {
		"@apollo/client": "^3.7.10",
		"@premia/ethers-multicall": "1.0.0-beta.13",
		"@premia/pair-lists": "^0.1.0",
		"@typechain/ethers-v6": "^0.3.2",
		"@types/sinon": "^10.0.14",
		"@types/ws": "^8.5.5",
		"@uniswap/permit2-sdk": "^1.2.0",
		"@uniswap/token-lists": "^1.0.0-beta.31",
		"@uqee/black-scholes": "^1.0.7",
		"axios": "^1.4.0",
		"cross-fetch": "^3.1.5",
		"dayjs": "^1.11.7",
		"dotenv": "^16.0.3",
		"extra-bigint": "^1.1.10",
		"graphql-query-to-json": "^2.0.1",
		"isomorphic-ws": "^5.0.0",
		"json-to-graphql-query": "^2.2.5",
		"localforage": "^1.10.0",
		"localforage-driver-memory": "^1.0.5",
		"lodash": "^4.17.21",
		"moment": "^2.29.4",
		"typechain": "^8.1.1"
	},
	"devDependencies": {
		"@nomicfoundation/hardhat-chai-matchers": "^2.0.0-beta.0",
		"@nomiclabs/hardhat-ethers": "^3.0.0-beta.0",
		"@typechain/hardhat": "^6.1.5",
		"@types/chai": "^4.3.4",
		"@types/lodash": "^4.14.191",
		"@types/mocha": "^10.0.1",
		"@types/node": "^18.14.2",
		"chai": "^4.3.7",
		"eslint-plugin-tsdoc": "^0.2.17",
		"eth-testing": "^1.12.0",
		"ethers": "^6.3.0",
		"hardhat": "^2.13.0",
		"husky": "^8.0.3",
		"jsdom": "^21.1.0",
		"jsdom-global": "^3.0.2",
		"mocha": "^10.2.0",
		"mock-local-storage": "^1.1.23",
		"prettier": "^2.8.4",
		"pretty-quick": "^3.1.3",
		"sinon": "^15.0.4",
		"ts-node": "^10.9.1",
		"typedoc": "^0.24.1",
		"typescript": "4.8.4"
	},
	"peerDependencies": {
		"ethers": ">=6 <7"
	},
	"optionalDependencies": {
		"graphql": "^16.6.0",
		"graphql-request": "^5.1.0"
	},
	"engines": {
		"node": ">=10"
	}
}
